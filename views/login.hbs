
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Login</title>
</head>
<header class="login">
    <div class="logo">N</div>
    <div class="bt"></div>
    <div class="ct"></div>
</header>
<div class="container-rl">
    <section class="login-sec">
        <h1>Login</h1>
        <form action="" method="post">
            <div class="input-form">
                <input type="email" name="email">
                <label>Email</label>
            </div>
            <div class="input-form">
                <input class="password1" type="password" name="password">
                <label>Contraseña</label>
                <img src="data:image/svg+xml;base64," class="open-password" onclick="document.querySelector('.close-password').style.display = 'grid';document.querySelector('.open-password').style.display = 'none';">
                <img src="data:image/svg+xml;base64," class="close-password" onclick="document.querySelector('.close-password').style.display = 'none';document.querySelector('.open-password').style.display = 'grid';">
            </div>
            <button type="submit">Login</button>
        </form>
    </section>
    <section class="register-sec">
        <h1>Register</h1>
        <form action="" method="post">
            <div class="input-reg">
                <input type="text" name="nombre">
                <label>Nombre</label>
            </div>
            <div class="input-reg">
                <input type="email" name="email">
                <label>Email</label>
            </div>
            <div class="input-reg">
                <input class="password2" type="password" name="password">
                <label>Contraseña</label>
                <img src="data:image/svg+xml;base64," class="open-pass" onclick="document.querySelector('.close-pass').style.display = 'grid';document.querySelector('.open-pass').style.display = 'none';">
                <img src="data:image/svg+xml;base64," class="close-pass" onclick="document.querySelector('.close-pass').style.display = 'none';document.querySelector('.open-pass').style.display = 'grid';">
            </div>
            <div class="input-reg">
                <input class="password3" type="password" name="cpassword">
                <label>Confirmar contraseña</label>
                <img src="data:image/svg+xml;base64," class="open-cpass" onclick="document.querySelector('.close-cpass').style.display = 'grid';document.querySelector('.open-cpass').style.display = 'none';">
                <img src="data:image/svg+xml;base64," class="close-cpass" onclick="document.querySelector('.close-cpass').style.display = 'none';document.querySelector('.open-cpass').style.display = 'grid';">
            </div>
            <div class="aceptar-terminos">
                <input type="checkbox" name="check">
                <p>Acepta nuestros <a href="/condiciones">terminos y condiciones</a></p>
            </div>
            <button type="submit">Register</button>
        </form>
    </section>
<script>
var p1 = document.querySelector('.password1');
var p2 = document.querySelector('.password2');
var p3 = document.querySelector('.password3');
window.addEventListener('click', function(e){
    var clase = e.target.className;
    if (clase === 'open-password'){
        document.querySelector('.password1').setAttribute('type','text');
    }
    if (clase === 'close-password'){
        document.querySelector('.password1').setAttribute('type','password');
    }
    if (clase === 'open-pass'){
        document.querySelector('.password2').setAttribute('type','text');
    }
    if (clase === 'close-pass'){
        document.querySelector('.password2').setAttribute('type','password');
    }
    if (clase === 'open-cpass'){
        document.querySelector('.password3').setAttribute('type','text');
    }
    if (clase === 'close-pass'){
        document.querySelector('.password3').setAttribute('type','password');
    }
});
</script>
</div>